{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "gwhthompson/laravel-cloudflare-transforms",
    "type": "library",
    "license": "MIT",
    "version": "1.0.0",
    "require": {
        "php": "^8.4",
        "illuminate/support": "^12.0",
        "illuminate/filesystem": "^12.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "phpstan/phpstan": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "Gwhthompson\\CloudflareTransforms\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gwhthompson\\CloudflareTransforms\\CloudflareTransformsServiceProvider"
            ]
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true
}
