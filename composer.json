{
  "$schema": "https://getcomposer.org/schema.json",
  "name": "gwhthompson/laravel-cloudflare-transforms",
  "description": "Fluent API for Cloudflare Image Transformation URLs in Laravel",
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/gwhthompson/laravel-cloudflare-transforms",
  "support": {
    "issues": "https://github.com/gwhthompson/laravel-cloudflare-transforms/issues",
    "source": "https://github.com/gwhthompson/laravel-cloudflare-transforms"
  },
  "keywords": [
    "laravel",
    "cloudflare",
    "images",
    "cdn",
    "transformations",
    "image-optimization",
    "responsive-images",
    "srcset",
    "storage",
    "media",
    "resize"
  ],
  "authors": [
    {
      "name": "George Thompson"
    }
  ],
  "require": {
    "php": "^8.3",
    "illuminate/support": "^11.0|^12.0",
    "illuminate/filesystem": "^11.0|^12.0"
  },
  "require-dev": {
    "laravel/boost": "^1.2",
    "driftingly/rector-laravel": "^2.0",
    "laravel/pint": "^1.0",
    "mockery/mockery": "^1.6",
    "orchestra/testbench": "^9.0|^10.0",
    "pestphp/pest": "^4.0",
    "pestphp/pest-plugin-laravel": "^4.0",
    "phpstan/phpstan": "^2.0",
    "rector/rector": "^2.0.0"
  },
  "autoload": {
    "psr-4": {
      "Gwhthompson\\CloudflareTransforms\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Gwhthompson\\CloudflareTransforms\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "./vendor/bin/pest",
    "lint": "./vendor/bin/pint",
    "analyse": "./vendor/bin/phpstan analyse"
  },
  "extra": {
    "laravel": {
      "providers": [
        "Gwhthompson\\CloudflareTransforms\\CloudflareTransformsServiceProvider"
      ],
      "aliases": {
        "CloudflareImage": "Gwhthompson\\CloudflareTransforms\\Facades\\CloudflareImage"
      }
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true,
    "allow-plugins": {
      "pestphp/pest-plugin": true,
      "php-http/discovery": true
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
